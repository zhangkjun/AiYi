<UserControl
    x:Class="AiYi.View.PrintSetView"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:control="clr-namespace:AiYi.Control"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:AiYi.View"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    d:DesignHeight="718"
    d:DesignWidth="924"
    DataContext="{Binding Source={StaticResource Locator}, Path=PrintSet}"
    mc:Ignorable="d">
    <StackPanel
        Height="50"
        Margin="50,60,0,0"
        VerticalAlignment="Top">
        <StackPanel Orientation="Horizontal">
            <TextBlock
                VerticalAlignment="Center"
                FontSize="16"
                Text="厨房是否打单" />
            <CheckBox
                Margin="20,0,0,0"
                VerticalAlignment="Center"
                IsChecked="{Binding IsIntimidate}"
                Style="{StaticResource SwitchCheckBox}" />
        </StackPanel>
    </StackPanel>
</UserControl>
