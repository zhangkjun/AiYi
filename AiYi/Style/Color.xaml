<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="clr-namespace:AiYi.Control">

    <!--  主题色  -->
    <SolidColorBrush x:Key="ThemeSolidColor" Color="#EF494D" />
    <Color x:Key="ThemeColor">#EF494D</Color>

    <!--  全白 透明色  -->
    <SolidColorBrush x:Key="TransparentColor" Color="#00FFFFFF" />
    <!--  全黑 透明色  -->
    <SolidColorBrush x:Key="TransparentBlackColor" Color="#00000000" />
</ResourceDictionary>